<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <filter>
        <filter-name>struts2</filter-name>
        <filter-class>org.apache.struts2.dispatcher.FilterDispatcher</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>struts2</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>ajaxData</servlet-name>
        <servlet-class>ajaxData</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ajaxIpChck</servlet-name>
        <servlet-class>ajaxIpChck</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ajaxIpChck2</servlet-name>
        <servlet-class>ajaxIpChck2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ajaxData</servlet-name>
        <url-pattern>/ajaxData</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ajaxIpChck</servlet-name>
        <url-pattern>/ajaxIpChck</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ajaxIpChck2</servlet-name>
        <url-pattern>/ajaxIpChck2</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            0
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
